<link rel="stylesheet" href="../styles/style.css">
<h1 class="header">TRACKS</h1>
<div>
    {{#each tracksList}}
    <article class="track">
    <h2>{{this.name}}</h2>
    <audio controls>
        <source src="{{this.preview_url}}" type="audio/mpeg">
        No audio supported in your browser
    </audio>
    </article>
    {{/each}}
</div>